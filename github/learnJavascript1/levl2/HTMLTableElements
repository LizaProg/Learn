/*htmlSelectElement*/
свойства                методы

                        add() добавление options
                        remove() удаление options
                        focus()
                        blur()
options(js) коллекция элементов в select
length(js) количество options
type (js) возвращает
selectedIndex  индекс выбранного элемента options
value
form
disabled
multiple
name
size
tabIndex


/*htmlTableElement*/

свойства           методы

tHead               createCaption()
tFoot               deleteCaptionns()
tBodies[]           createTHead()
rows[]              deleteTHead()
caption             createTFoot()
width               deleteTFoot()
border              insertRow(index)
cellPadding         deleteRow(index)
cellSpasing

/*htmlTableSectionElement*/

свойства            методы

rows                insertRow(index)
align               deleteRow(index)
vAlign

/*htmlTableRowElement*/

свойства                    методы

cells                       insertCell(index)
rowindex                    deleteCell(index)
sectionRowindex
vAlign
bgColor

